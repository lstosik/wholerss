<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xmlns="http://java.sun.com/xml/ns/javaee"
		 xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
		 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
		 version="2.5">
	<servlet>
		<servlet-name>RSSFullViewServlet</servlet-name>
		<servlet-class>net.purevirtual.fullrss.RSSFullViewServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>WicketServlet</servlet-name>
		<servlet-class>org.apache.wicket.protocol.http.WicketServlet</servlet-class>
		<init-param>
			<param-name>applicationClassName</param-name>
			<param-value>net.purevirtual.fullrss.FullRssApp</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>RSSFullViewServlet</servlet-name>
		<url-pattern>/rss/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>WicketServlet</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>
</web-app>